  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:staged": "lint-staged",
    "test": "jest --config ./jest.config.js ./src",
    "start": "NODE_ENV=development nodemon src --exec babel-node",
    "db:seed": "babel-node scripts/run-db-seed.js",
    "db:clear": "babel-node scripts/run-db-clear.js",
    "db:setup": "npm run db:clear && npm run db:seed",
    "build": "babel server -d build/src",
    "execute": "yarn pm2 start build/src --name express",
    "build-and-execute": "concurrently \"yarn execute\" \"yarn build\"",
    "jsdoc": "jsdoc src -r -c .jsdoc.json -d docs"
  },
  "pre-commit": "lint:staged",
  "lint-staged": {
    "*.{md,sh,txt,xml,html,json}": [
      "editorconfig-tools fix",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
